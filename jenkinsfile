def ip = ""


pipeline{
    agent any 
    tools {
        terraform  'terraform-11'
    }

    stages{
     
        
        stage("terraform"){
            

            steps{
                
                script{


                
                sh " terraform init"
              
                }
            
            }
        }
        stage('terraform apply') {
            steps{
                script{
                sh " terraform apply --auto-approve "
                }
                
        }
        
        
 }
}
